{
  "name": "expenses-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx run client:serve --host=127.0.0.1",
    "postinstall": "node ./decorate-angular-cli.js",
    "build:stats": "nx run client:build:development --stats-json --source-map",
    "build": "nx run client:build:production",
    "analyze": "source-map-explorer dist/apps/client/**/*.js",
    "emulator:export": "firebase emulators:export --only=auth,firestore ./backup",
    "emulator:import": "firebase emulators:start --import=./backup"
  },
  "private": true,
